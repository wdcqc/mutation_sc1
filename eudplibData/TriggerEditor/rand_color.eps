import PluginVariables as msqcvar;
const colors = [111,20,12,14,164,135,137,191,134,59,159,160,173,146,123,156,85,186];
var color_len = 18;

function randomize_comp_colors() {
	for(var player = 0; player < 8; player++) {
		const pcolor_idx = dwrand() % color_len;
		const pcolor = colors[pcolor_idx];
		SetMemoryXEPD(EPD(5774708) + player * 2, SetTo, pcolor * 65536, 0xff0000);
		bwrite(5774806 + player, pcolor);
		colors[pcolor_idx] = colors[color_len - 1];
		color_len -= 1;
	}
}